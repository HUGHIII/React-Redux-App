{
    "version": 2,
    "name": "async-redux-react-cra",
    "builds": [
      { 
       "src": "async-redux-react-cra/package.json", 
       "use": "@now/static-build", 
       "config": {
          "distDir": "build"
          } 
       }
      ],
    "routes": [
        { 
           "src": "/(.*)", 
           "dest": "async-redux-react-cra/$1" 
        }
    ]
}